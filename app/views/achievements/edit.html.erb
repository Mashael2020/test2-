<h1>Achievements#edit</h1>
<p>Find me in app/views/achievements/edit.html.erb</p>

<%= form_for @achievement, url: {action: "update"} do |f|%>
    



    <div class="field">
    <%= f.label :achievement%> <br />
    <%= f.text_field :achievement , :required=> true%>
  </div>
  
       <div class="field">
         <%= f.label :note%><br />
         <%= f.text_field :note, :required=> true %>
       </div>
      
       <div class="field">
         <%= f.label :date %><br />
         <%= f.date_field :date , :required=> true %> 
       </div>
       <div class="field">
         <%= f.label :image %><br />
         <%= f.file_field :image, :required=> true %>
       </div>
      
       <div class="actions">
         <%= f.submit "update" %>
       </div>
      <% end %> 
  